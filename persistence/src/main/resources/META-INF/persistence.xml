<?xml version="1.0"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
    <persistence-unit name="test">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>com.messio.appengine.persistence.entity.Decision</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/darts"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.user" value="darts"/>
            <property name="javax.persistence.jdbc.password" value="target"/>
        </properties>
    </persistence-unit>
</persistence>